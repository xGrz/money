{
    "name": "xgrz/money",
    "description": "Money helper for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "money"
    ],
    "autoload": {
        "psr-4": {
            "xGrz\\Money\\": "src/"
        },
        "files": [
            "src/global-helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "xGrz\\Money\\MoneyServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Grzegorz By≈õkiniewicz",
            "email": "grzesiek.byskiniewicz@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
