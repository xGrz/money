{
  "name": "xgrz/money",
  "description": "Money helper for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "money",
    "helper"
  ],
  "autoload": {
    "psr-4": {
      "xGrz\\Money\\": "src/"
    },
    "files": [
      "src/global-helper.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "xGrz\\Money\\MoneyServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Grzegorz By≈õkiniewicz",
      "email": "grzesiek.byskiniewicz@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
    "phpunit/phpunit": "^10.0 || ^11.0",
    "phpstan/phpstan": "^1.0 || ^2.0",
    "larastan/larastan": "^2.0 || ^3.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "scripts": {
    "qtest": "phpunit --colors --testdox",
    "qtestc": "phpunit --colors --testdox --coverage-text",
    "phpstan": "vendor/bin/phpstan --configuration=phpstan.neon --memory-limit=2G",
    "test": [
      "@qtestc",
      "@phpstan"
    ]
  }
}
